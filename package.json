    "gatsby-plugin-image": "^2.22.0",
    "gatsby-plugin-manifest": "^4.12.1",
    "gatsby-plugin-mdx": "^3.15.0",
    "gatsby-plugin-react-helmet": "^5.22.0",
    "gatsby-plugin-sitemap": "^5.12.1",
    "gatsby-plugin-theme-ui": "^0.14.7",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "gatsby-source-filesystem": "^4.22.0",
    "gatsby-transformer-sharp": "^4.22.0",
    "gh-pages": "^4.0.0",
    "node-fetch": "^2.6.7",
    "polished": "^4.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-spring": "^9.5.2",
    "theme-ui": "^0.11.3"
